{
  "name": "record-stack-overflow",
  "version": "1.0.0",
  "description": "REST API resume project",
  "main": "app.js",
  "scripts": {
    "clean": "rm -rf build && mkdir build",
    "build-babel": "babel -d ./build ./src -s",
    "build": "npm run clean && npm run build-babel",
    "start": "npm run build && node ./build/app.js",
    "dev": "nodemon src/app.js --exec babel-node -e js",
    "test": "export NODE_ENV=test && mocha server/**/*.test.js"
  },
  "author": "Ryan Seymour",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "^3.0.3",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.18.3",
    "crypto-js": "^3.1.9-1",
    "ejs": "^2.6.1",
    "express": "^4.16.4",
    "express-rate-limit": "^3.3.2",
    "helmet": "^3.15.0",
    "jsonwebtoken": "^8.4.0",
    "mongoose": "^5.4.2",
    "morgan": "^1.9.1",
    "nodemon": "^1.18.9",
    "validator": "^10.11.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-env": "^1.7.0"
  },
  "engines": {
    "node": "11.0.0",
    "npm": "6.5.0"
  }
}